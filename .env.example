# Environment Variables Example
# Copy this file to .env and fill with your actual values

# Mobile API Base URL
MOBILE_API_BASE=https://bisma.bekasikab.go.id/api

# Optional: API Key if mobile backend requires it (currently not needed)
# MOBILE_API_KEY=your_api_key_here

# Optional: Internal API endpoints (for production)
# INTERNAL_API_BASE=http://internal-server/api

# Demo Mode
DEMO_MODE=true

# Security
# SESSION_SECRET=your_random_secret_here
# JWT_SECRET=your_jwt_secret_here

# Logging
LOG_LEVEL=info
